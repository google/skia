half H = 1.0;
float F = 1.0;
half4 H4 = half4(1.0);
float4 F4 = float4(1.0);
half4x4 H4x4 = half4x4(1.0);
float4x4 F4x4 = float4x4(1.0);

void scalar_times_scalar_ok_1()         { F = F * H; }
