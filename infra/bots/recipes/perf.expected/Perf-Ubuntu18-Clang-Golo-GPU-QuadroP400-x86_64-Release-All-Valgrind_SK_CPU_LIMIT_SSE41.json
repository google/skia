      "1.1",
      "--nocpu",
      "--config",
      "gl",
      "glsrgb",
      "glmsaa8",
      "--loops",
      "1",
      "--samples",
      "1",
      "--keepAlive",
      "true",
      "--match",
      "~inc0.gif",
      "~inc1.gif",
      "~incInterlaced.gif",
      "~inc0.jpg",
      "~incGray.jpg",
      "~inc0.wbmp",
      "~inc1.wbmp",
      "~inc0.webp",
      "~inc1.webp",
      "~inc0.ico",
      "~inc1.ico",
      "~inc0.png",
      "~inc1.png",
      "~inc2.png",
      "~inc12.png",
      "~inc13.png",
      "~inc14.png",
      "~inc0.webp",
      "~inc1.webp"
    ],
    "env": {
      "CHROME_HEADLESS": "1",
      "PATH": "<PATH>:RECIPE_REPO[depot_tools]",
      "VALGRIND_LIB": "[START_DIR]/valgrind/lib/valgrind"
    },
    "name": "nanobench"
  },
