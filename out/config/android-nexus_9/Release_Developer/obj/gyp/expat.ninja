defines = -DSK_INTERNAL -DSK_GAMMA_APPLY_TO_A8 $
    -DSK_ALLOW_STATIC_GLOBAL_INITIALIZERS=0 -DSK_SUPPORT_GPU=1 $
    -DSK_SUPPORT_OPENCL=0 -DSK_FORCE_DISTANCE_FIELD_TEXT=0 $
    -DSK_BUILD_FOR_ANDROID -DSK_GAMMA_EXPONENT=1.4 -DSK_GAMMA_CONTRAST=0.0 $
    -DSKIA_DLL -DSKIA_IMPLEMENTATION=1 -DSK_SUPPORT_LEGACY_CLIPTOLAYERFLAG $
    -DHAVE_EXPAT_CONFIG_H -DNDEBUG -DSK_DEVELOPER=1
includes = $
    -I../../../../../../paintLib/skia/platform_tools/android/third_party/externals/expat $
    -I../../../../../../paintLib/skia/platform_tools/android/third_party/externals/expat/lib
cflags = -fPIC -g -fno-exceptions -fstrict-aliasing -Wall -Wextra $
    -Winit-self -Wpointer-arith -Wsign-compare -Wno-unused-parameter $
    -fuse-ld=gold -w -fexceptions -O2
cflags_c = 
cflags_cc = -std=c++11 -fno-rtti -fno-threadsafe-statics -Wnon-virtual-dtor
arflags = 

build $
    obj/platform_tools/android/third_party/externals/expat/lib/expat.xmlparse.o: $
    cc $
    ../../../../../../paintLib/skia/platform_tools/android/third_party/externals/expat/lib/xmlparse.c
build $
    obj/platform_tools/android/third_party/externals/expat/lib/expat.xmlrole.o: $
    cc $
    ../../../../../../paintLib/skia/platform_tools/android/third_party/externals/expat/lib/xmlrole.c
build $
    obj/platform_tools/android/third_party/externals/expat/lib/expat.xmltok.o: $
    cc $
    ../../../../../../paintLib/skia/platform_tools/android/third_party/externals/expat/lib/xmltok.c

build obj/gyp/libexpat.a: alink_thin $
    obj/platform_tools/android/third_party/externals/expat/lib/expat.xmlparse.o $
    obj/platform_tools/android/third_party/externals/expat/lib/expat.xmlrole.o $
    obj/platform_tools/android/third_party/externals/expat/lib/expat.xmltok.o
