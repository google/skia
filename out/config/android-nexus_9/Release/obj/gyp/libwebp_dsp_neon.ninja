defines = -DSK_INTERNAL -DSK_GAMMA_APPLY_TO_A8 $
    -DSK_ALLOW_STATIC_GLOBAL_INITIALIZERS=0 -DSK_SUPPORT_GPU=1 $
    -DSK_SUPPORT_OPENCL=0 -DSK_FORCE_DISTANCE_FIELD_TEXT=0 $
    -DSK_BUILD_FOR_ANDROID -DSK_GAMMA_EXPONENT=1.4 -DSK_GAMMA_CONTRAST=0.0 $
    -DSKIA_DLL -DSKIA_IMPLEMENTATION=1 -DSK_SUPPORT_LEGACY_CLIPTOLAYERFLAG $
    -DHAVE_CONFIG_H -DWEBP_SWAP_16BIT_CSP -DNDEBUG
includes = -I../../../../third_party/libwebp/webp $
    -I../../../../third_party/externals/libwebp
cflags = -fPIC -g -fno-exceptions -fstrict-aliasing -Wall -Wextra $
    -Winit-self -Wpointer-arith -Wsign-compare -Wno-unused-parameter $
    -fuse-ld=gold -w -O2
cflags_c = 
cflags_cc = -std=c++11 -fno-rtti -fno-threadsafe-statics -Wnon-virtual-dtor
arflags = 

build $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.dec_neon.o: $
    cc ../../../../third_party/externals/libwebp/src/dsp/dec_neon.c
build $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.enc_neon.o: $
    cc ../../../../third_party/externals/libwebp/src/dsp/enc_neon.c
build $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.lossless_neon.o: $
    cc ../../../../third_party/externals/libwebp/src/dsp/lossless_neon.c
build $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.lossless_enc_neon.o: $
    cc ../../../../third_party/externals/libwebp/src/dsp/lossless_enc_neon.c
build $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.upsampling_neon.o: $
    cc ../../../../third_party/externals/libwebp/src/dsp/upsampling_neon.c

build obj/gyp/libwebp_dsp_neon.a: alink_thin $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.dec_neon.o $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.enc_neon.o $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.lossless_neon.o $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.lossless_enc_neon.o $
    obj/third_party/externals/libwebp/src/dsp/libwebp_dsp_neon.upsampling_neon.o
