    -I../../../../../../paintLib/skia/src/utils $
    -I../../../../../../paintLib/skia/include/gpu $
    -I../../../../../../paintLib/skia/third_party/externals/jsoncpp-chromium/overrides/include $
    -I../../../../../../paintLib/skia/third_party/externals/jsoncpp/include $
    -I../../../../../../paintLib/skia/src/gpu $
    -I../../../../../../paintLib/src/gpu $
    -I../../../../../../paintLib/skia/tools
cflags = -fPIC -g -fno-exceptions -fstrict-aliasing -Wall -Wextra $
    -Winit-self -Wpointer-arith -Wsign-compare -Wno-unused-parameter $
    -Werror -fuse-ld=gold -g
cflags_c = 
cflags_cc = -std=c++11 -fno-rtti -fno-threadsafe-statics -Wnon-virtual-dtor
arflags = 

build obj/tools/render_pictures.render_pictures_main.o: cxx $
    ../../../../../../paintLib/skia/tools/render_pictures_main.cpp

ldflags = 
libs = -llog
build lib/librender_pictures.so lib/librender_pictures.so.TOC: solink $
    obj/tools/render_pictures.render_pictures_main.o $
    obj/gyp/libandroid_output.a obj/gyp/libflags.a $
    obj/gyp/libpicture_renderer.a obj/gyp/libpicture_utils.a $
    obj/gyp/libjsoncpp.a libskia_skgputest.a | lib/libskia_android.so.TOC $
    skia_launcher
  ld = $ldxx
  soname = librender_pictures.so
  lib = lib/librender_pictures.so
  link_file_list = lib/librender_pictures.so.rsp
  solibs = lib/libskia_android.so
