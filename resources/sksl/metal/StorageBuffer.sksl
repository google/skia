// TODO: Move this test to shared once storage buffers are supported in other backends
struct SomeData {
    float4 a;
    float2 b;
};

layout(set=0, binding=0) readonly buffer storageBuffer
{
    uint offset;
    SomeData[] inputData;
};

half4 main(float2 coords) {
    return half4(inputData[offset].a * inputData[offset].b.x);
}
