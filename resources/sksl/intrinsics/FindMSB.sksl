uniform int a;
uniform uint b;
void main() {
    int b1 = findMSB(a) + findMSB(b);
    int2 b2 = findMSB(int2(a)) + findMSB(uint2(b));
    int3 b3 = findMSB(int3(a)) + findMSB(uint3(b));
    int4 b4 = findMSB(int4(a)) + findMSB(uint4(b));

    sk_FragColor = half4(b1.xxxx + b2.xyxy + b3.xyz1 + b4);
}
