{
   "MaxCount": 4096,
   "Drawable": {
      "Type": "SkCircleDrawable",
      "Radius": 2
   },
   "EffectCode": [
      "void effectSpawn(inout Effect effect) {",
      "  effect.rate = 90;",
      "}",
      ""
   ],
   "Code": [
      "float2 circle() {",
      "  float x;",
      "  float y;",
      "  do {",
      "    x = rand * 2 - 1;",
      "    y = rand * 2 - 1;",
      "  } while (x*x + y*y > 1);",
      "  return float2(x, y);",
      "}",
      "",
      "void spawn(inout Particle p) {",
      "  p.lifetime = 30;",
      "  p.pos += circle() * 40;",
      "}",
      "",
      "void update(inout Particle p) {",
      "  p.vel += normalize(p.pos - effect.pos) * dt * 10;",
      "  p.scale = mix(0.25, 3, p.age);",
      "}",
      ""
   ],
   "Bindings": []
}